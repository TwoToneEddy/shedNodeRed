    {
        "id": "32f82a54.c51a06",
        "type": "function",
        "z": "76b0c291.39da1c",
        "name": "disable_solar_when_battery_full",
        "func": "var battery_voltage = global.get('battery_voltage');\nvar vmax = 12;\nvar hyst = 0.2;\nvar auto_disconnect_pv;\n\nif(global.get('auto')==true){\n    if(battery_voltage > vmax){\n        global.set('auto_disconnect_pv',true);\n    }\n    if(battery_voltage < (vmax-hyst)){\n        global.set('auto_disconnect_pv',false);\n    }\n    \n    \n    msg.payload = global.get('auto_disconnect_pv');\n}else{\n    global.set('auto_disconnect_pv',false);\n    msg.payload = true;\n}\n\nreturn msg;",
